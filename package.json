{
	"name": "svelte-kit-template",
	"version": "0.0.1",
	"description": "SvelteKit tempalte",
	"keywords": [
		"portable text",
		"sanity",
		"svelte",
		"sveltekit",
		"tailwindcss"
	],
	"homepage": "https://github.com/accuser/svelte-kit-template/README.md",
	"bugs": {
		"url": "https://github.com/accuser/svelte-kit-template/issues"
	},
	"license": "MIT",
	"author": {
		"name": "Matthew Gibbons",
		"url": "https://github.com/accuser"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/accuser/svelte-kit-template.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"test": "vitest run",
		"test:watch": "vitest",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@portabletext/svelte": "^1.0.1",
		"@sanity/client": "^3.3.3",
		"@sveltejs/adapter-auto": "1.0.0-next.64",
		"@sveltejs/kit": "1.0.0-next.405",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tailwindcss/typography": "^0.5.4",
		"@testing-library/svelte": "^3.1.3",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"@vitest/ui": "^0.21.0",
		"autoprefixer": "^10.4.8",
		"cssnano": "^5.1.12",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"eslint-plugin-tailwindcss": "^3.6.0",
		"groq": "^2.29.3",
		"postcss": "^8.4.16",
		"postcss-nesting": "^10.1.10",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "~4.7.4",
		"vite": "^3.0.4",
		"vitest": "^0.21.0"
	},
	"type": "module"
}
